public class UjiPraktikum1 {
    public static void main(String[] nisa) {
        
        
    Praktikum1 Praktikum1 = new Praktikum1(20);
        Praktikum1.getPenumpang(12);
        Praktikum1.getPenumpang(123.0);
        Praktikum1.cetakpenumpang();
        System.out.println();
        
    //Panambahan penumpang
   Praktikum1.addPenumpang (2);//tambah 2 penumpang
    Praktikum1.cetakpenumpang();
    System.out.println();
    
    Praktikum1.addPenumpang (2);//tambah 2 penumpang
    Praktikum1.cetakpenumpang();
    System.out.println();
    
   Praktikum1.addPenumpang (5);//tambah 2 penumpang
   Praktikum1.cetakpenumpang();
    System.out.println();
    
    Praktikum1.addPenumpang (8);//tambah 2 penumpang
    Praktikum1.cetakpenumpang();
    System.out.println();
    
    Praktikum1.addPenumpang (10);//tambah 2 penumpang
    Praktikum1.cetakpenumpang();
    System.out.println();
    
    System.out.println("Rata-rata Penumpang Bus Sekarang adalah = " 
            + Praktikum1.getAverage()); 
}
}   
